import Property

class House(Property):
    def __init__(self, area, rooms, price, address, plot):
        self.area = area
        self.rooms = rooms
        self.price = price
        self.address = address
        self.plot = plot
        
    def __str__(self):
        return f"{self.area}\n{self.rooms}\n{self.price}\n{self.address}\n{self.plot}"